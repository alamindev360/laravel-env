{
    "name": "alamindev360/laravel-env",
    "description": "Easily update or insert Laravel environment variable with Artisan command",
    "type": "laravel-package",
    "keywords": [
        "alamindev360",
        "laravel",
        "set",
        "env",
        "programatically",
        "command",
        "artisan",
        "php"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AlaminDev360\\SetEnv\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Al Amin",
            "email": "alamindev360@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alamindev360/laravel-env"
        }
    ],
    "require": {
        "alamindev36/laravel-env": "dev-main",
        "php": "^8.1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AlaminDev360\\SetEnv\\Providers\\SetEnvServiceProvider"
            ]
        }
    }
}

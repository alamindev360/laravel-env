{
    "name": "alamindev360/laravel-env",
    "description": "Easily update or insert Laravel environment variable with Artisan command",
    "type": "laravel-package",
    "keywords": [
        "syamsoul",
        "laravel",
        "set",
        "env",
        "programatically",
        "command",
        "artisan",
        "php",
        "souldoit"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SoulDoit\\SetEnv\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Al Amin",
            "email": "alamindev360@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoulDoit\\SetEnv\\Providers\\SetEnvServiceProvider"
            ]
        }
    }
}
